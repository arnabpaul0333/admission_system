<h1>Academics Details</h1>
<div class="row">
  <div class="span6 offset3">
    <%= form_for [@student, @education] do |f| %>
      <%= f.label :tenth_board %>
      <%= f.text_field :tenth_board %>
   
      <%= f.label :tenth_school_name %>
      <%= f.text_field :tenth_school_name %>

      <%= f.label :tenth_percentage %>
      <%= f.number_field :tenth_percentage %>

      <%= f.label :twelvth_board %>
      <%= f.text_field :twelvth_board %>

      <%= f.label :twelvth_school_name %>
      <%= f.text_field :twelvth_school_name %>

      <%= f.label :twelvth_percentage %>
      <%= f.number_field :twelvth_percentage %>
      
      <%= f.label :entrance_rank %>
      <%= f.number_field :entrance_rank %><br>
    
      <%= f.submit "Next Page", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
